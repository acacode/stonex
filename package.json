{
  "name": "stonex",
  "version": "0.0.4-alpha",
  "description": "Predictable state container for TypeScript/JavaScript applications",
  "main": "dist/index.js",
  "scripts": {
    "_prepublish": "yarn build",
    "build:live": "nodemon --legacy-watch --watch \".\\**\\*.ts\" --exec ts-node example/index.ts",
    "build": "tsc -p .",
    "exec": "ts-node example.ts",
    "lint": "tslint 'lib/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "test": "echo \"Error: no test specified\" && exit 0",
    "debug": "npm run build:live --debug-brk=5858",
    "stage-dist-changes": "git add dist",
    "release": "release-it"
  },
  "pre-commit": [
    "lint",
    "build",
    "stage-dist-changes"
  ],
  "author": "acacode",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.18.10",
    "pre-commit": "^1.2.2",
    "release-it": "9.2.0",
    "ts-node": "^8.0.2",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint": "5.13.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {},
  "typings": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acacode/stonex.git"
  },
  "keywords": [
    "state",
    "predictable",
    "modular",
    "immutable",
    "react",
    "state-container",
    "javascript",
    "javascript-library",
    "redux",
    "reducer",
    "flux"
  ]
}
